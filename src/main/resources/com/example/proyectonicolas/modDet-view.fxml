      <TextField fx:id="nameBox" layoutX="79.0" layoutY="55.0" prefHeight="26.0" prefWidth="186.0" promptText="Nombre" />
      <TextField fx:id="dateBox" layoutX="79.0" layoutY="104.0" prefHeight="26.0" prefWidth="186.0" promptText="Lanzamiento AAAA-MM-DD" />
      <Button fx:id="resetButton" layoutX="248.0" layoutY="328.0" mnemonicParsing="false" onAction="#reset" text="Reset" />
      <Button fx:id="cancelButton" layoutX="146.0" layoutY="328.0" mnemonicParsing="false" onAction="#closeWindow" text="Cancelar" />
      <Button fx:id="addNewButton" layoutX="364.0" layoutY="328.0" mnemonicParsing="false" onAction="#addNew" text="Actualizar" />
      <ToggleButton fx:id="deptvTogle" layoutX="364.0" layoutY="152.0" mnemonicParsing="false" onAction="#cambioDep" prefHeight="26.0" prefWidth="56.0" text="No" />
