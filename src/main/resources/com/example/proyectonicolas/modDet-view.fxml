<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectonicolas.controllers.ModDetController">
   <children>
      <TextField fx:id="comBox" layoutX="79.0" layoutY="200.0" prefHeight="26.0" prefWidth="186.0" promptText="País de fabricación" />
